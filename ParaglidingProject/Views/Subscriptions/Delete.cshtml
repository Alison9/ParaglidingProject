@model ParaglidingProject.Models.Subscription

@{
    ViewData["Title"] = "Delete";
}

<h1>Supprimer l'année de cotisation @Model.YearID?</h1>

<h3>Etes-vous sûr de vouloir supprimer?</h3>
<div>
    <h4>Subscription</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Price)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Price)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="YearID" />
        <input type="submit" value="Supprimer" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-primary">Retour</a>
    </form>
</div>
