@model ParaglidingProject.Models.Paraglider

@{
    ViewData["Title"] = "Delete";
}

<h1>Supprimer</h1>

<h3>Etes-vous sûr de vouloir supprimer ce parapente ?</h3>
<div>
    <h4>Paraglider</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CommissioningDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CommissioningDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.LastRevisionDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LastRevisionDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ParagliderModelID)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ParagliderModelID)
        </dd class>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="ID" />
        <input type="submit" value="Supprimer" class="btn btn-danger" /> |
        <a class="btn btn-primary" asp-action="Index">Retour à la liste</a>
    </form>
</div>
